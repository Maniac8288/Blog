<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>Metronic | Page Layouts - Blank Page</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta http-equiv="Content-type" content="text/html; charset=utf-8">
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href='@Url.Content("~/Content/themes/base/all.css")' rel="stylesheet" type="text/css" />

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css">
    <link href="~/Areas/Admin/Metrionic/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/Admin/Metrionic/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/Admin/Metrionic/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/Admin/Metrionic/global/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/Admin/Metrionic/global/plugins/ckeditor/skins/moono-lisa/editor.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/Admin/Metrionic/global/plugins/bootstrap-markdown/css/bootstrap-markdown.min.css" rel="stylesheet" type="text/css">
    <link href="~/Areas/Admin/Metrionic/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/Admin/Metrionic/global/plugins/jquery-tags-input/jquery.tagsinput.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/Admin/Metrionic/global/plugins/typeahead/typeahead.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN THEME STYLES -->
    <link href="~/Areas/Admin/Metrionic/global/css/components-md.css" id="style_components" rel="stylesheet" type="text/css" />
    <link href="~/Areas/Admin/Metrionic/global/css/plugins-md.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/Admin/Metrionic/admin/layout4/css/layout.css" rel="stylesheet" type="text/css" />
    <link id="style_color" href="~/Areas/Admin/Metrionic/admin/layout4/css/themes/light.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/Admin/Metrionic/admin/layout4/css/custom.css" rel="stylesheet" type="text/css" />
    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-md page-header-fixed page-sidebar-closed-hide-logo">
    <!-- BEGIN HEADER -->
    <div class="page-header md-shadow-z-1-i navbar navbar-fixed-top">
        <!-- BEGIN HEADER INNER -->
        <div class="page-header-inner">
            <!-- BEGIN LOGO -->
            <div class="page-logo">
                <a href="@Url.Action("Index", "Dashboard")">
                    <img src="~/Areas/Admin/Metrionic/admin/layout4/img/logo-light.png" alt="logo" class="logo-default" />
                </a>
                <div class="menu-toggler sidebar-toggler">
                    <!-- DOC: Remove the above "hide" to enable the sidebar toggler button on header -->
                </div>
            </div>
            <!-- END LOGO -->
            <!-- BEGIN RESPONSIVE MENU TOGGLER -->
            <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse">
            </a>
            <!-- END RESPONSIVE MENU TOGGLER -->
            <!-- BEGIN PAGE TOP -->
            <div class="page-top">
            </div>
            <!-- END PAGE TOP -->
        </div>
        <!-- END HEADER INNER -->
    </div>
    <!-- END HEADER -->
    <div class="clearfix">
    </div>
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN SIDEBAR -->
       @Html.Partial("SideBar")
        <!-- END SIDEBAR -->
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
            <div class="page-content">
              
           
                <!-- BEGIN PAGE CONTENT-->
                <div class="row">
                    <div class="col-md-12">
                        @RenderBody()
                    </div>
                </div>
                <!-- END PAGE CONTENT-->
            </div>
        </div>
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    
    <!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
    <!-- BEGIN CORE PLUGINS -->
    <!--[if lt IE 9]>
    <script src="../../assets/global/plugins/respond.min.js"></script>
    <script src="../../assets/global/plugins/excanvas.min.js"></script>
    <![endif]-->
    <script src="~/Areas/Admin/Metrionic/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/jquery-migrate.min.js" type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
    <script src="~/Areas/Admin/Metrionic/global/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/jquery.cokie.min.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/uniform/jquery.uniform.min.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <script src="~/Areas/Admin/Metrionic/global/plugins/typeahead/typeahead.bundle.js"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/jquery-tags-input/jquery.tagsinput.js"></script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/ckeditor/ckeditor.js" type="text/javascript"> </script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/ckeditor/config.js" type="text/javascript"> </script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/bootstrap-markdown/lib/markdown.js" type="text/javascript"> </script>
    <script src="~/Areas/Admin/Metrionic/global/plugins/bootstrap-markdown/js/bootstrap-markdown.js" type="text/javascript"> </script>
    <script src="~/Areas/Admin/Metrionic/global/scripts/metronic.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/admin/layout4/scripts/layout.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Metrionic/admin/layout4/scripts/demo.js" type="text/javascript"></script>
  <script src="~/Scripts/jquery-ui-1.12.1.js" type="text/javascript"></script>
    <script>
      jQuery(document).ready(function() {
         Metronic.init(); // init metronic core components
Layout.init(); // init current layout
Demo.init(); // init demo features
      });
    </script>
    <!-- END JAVASCRIPTS -->
    @RenderSection("scripts", required: false)
</body>
<!-- END BODY -->
</html>